{
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "name": "@moukail/angularx-qrcode",
  "version": "9.0.0",
  "author": {
    "name": "Andreas Jacob <andreas@cordobo.de>",
    "url": "http://cordobo.com/"
  },
  "description": "Simple QRCode module generator for Angular9+",
  "license": "MIT",
  "scripts": {
    "build": "ng-packagr -p ng-package.json",
    "lint": "tslint 'src/**/*.ts' --fix",
    "prebuild": "rimraf dist tmp",
    "reinstall": "rm -rf node_modules && npm install",
    "release": "npm run reinstall && npm run build && cd dist/angularx-qrcode && npm publish --access public && cd ../../"
  },
  "dependencies": {
    "@angular/common": "^9.0.0-rc.14",
    "qrcode": "1.4.4"
  },
  "peerDependencies": {
    "@angular/common": "^9.0.0-rc.14",
    "@angular/core": "^9.0.0-rc.14"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.900.0-rc.12",
    "@angular-devkit/build-ng-packagr": "^0.900.0-rc.12",
    "@angular/cli": "^9.0.0-rc.12",
    "@angular/compiler": "^9.0.0-rc.14",
    "@angular/compiler-cli": "^9.0.0-rc.14",
    "@angular/core": "^9.0.0-rc.14",
    "@angular/language-service": "^9.0.0-rc.14",
    "@angular/platform-browser": "^9.0.0-rc.14",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.14",
    "@types/node": "^13.1.4",
    "@types/qrcode": "^1.3.4",
    "codelyzer": "5.2.1",
    "ng-packagr": "5.7.1",
    "rimraf": "3.0.1",
    "rxjs": "^6.5.4",
    "ts-node": "8.5.4",
    "tslib": "1.10.0",
    "tslint": "5.20.1",
    "typescript": "~3.6.4",
    "zone.js": "0.10.2"
  },
  "keywords": [
    "angularx-qrcode",
    "angular-qrcode",
    "ng-qrcode",
    "ngx-qrcode",
    "angular",
    "angular4",
    "angular5",
    "angular6",
    "angular7",
    "angular8",
    "angular9",
    "ionic",
    "ionic3",
    "ionic4",
    "aot",
    "aot-compatible",
    "aot-compilation",
    "library",
    "ng",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/moukail/angularx-qrcode.git"
  },
  "bugs": {
    "url": "https://github.com/moukail/angularx-qrcode/issues"
  },
  "homepage": "https://github.com/moukail/angularx-qrcode#readme"
}
